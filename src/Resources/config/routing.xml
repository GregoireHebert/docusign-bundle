<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="docusign_sign" path="docusign" methods="POST|GET">
        <default key="_controller">docusign_sign</default>
    </route>

    <route id="docusign_callback" path="docusign/callback" methods="GET">
        <default key="_controller">docusign_callback</default>
    </route>

    <route id="docusign_webhook" path="docusign/webhook" methods="POST">
        <default key="_controller">docusign_webhook</default>
    </route>

</routes>
