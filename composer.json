{
    "name": "gheb/docusign-bundle",
    "description": "Symfony Bundle for electronic document signature with Docusign",
    "homepage": "https://github.com/GregoireHebert/docusign-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony","symfony-bundle","electronic signature", "docusign"],
    "readme": "README.md",
    "authors": [
        {
            "name": "Grégoire Hébert",
            "email": "gregoire@les-tilleuls.coop",
            "homepage": "https://knot.gheb.dev/",
            "role": "Developer"
        },
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "gregoire+support-docusign@les-tilleuls.coop"
    },
    "require": {
        "php": "^7.2",
        "ext-SimpleXML": "*",
        "docusign/esign-client": "^3.0",
        "lcobucci/jwt": "^3.3.1",
        "league/flysystem": "^1.0.8",
        "psr/log": "^1.1",
        "symfony/config": "^3.4 || ^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/event-dispatcher": "^3.4 || ^4.2",
        "symfony/http-client": "^3.4 || ^4.2",
        "symfony/http-foundation": "^3.4 || ^4.2",
        "symfony/http-kernel": "^3.4 || ^4.2",
        "symfony/options-resolver": "^3.4 || ^4.2",
        "symfony/routing": "^3.4 || ^4.2",
        "symfony/stopwatch": "^3.4 || ^4.2",
        "symfony/translation": "^3.4 || ^4.3",
        "vgrem/php-spo": "^2.2",
        "webmozart/assert": "^1.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "league/flysystem-bundle": "^1.2",
        "nyholm/symfony-bundle-test": "^1.4",
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^7.5.2 || ^8.0",
        "symfony/css-selector": "^3.4 || ^4.2",
        "symfony/dotenv": "^3.4 || ^4.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/panther": "^0.6.0",
        "symfony/polyfill-php72": "^1.9",
        "symfony/process": "^3.4 || ^4.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^3.4 || ^4.2",
        "symfony/debug": "^3.4 || ^4.2",
        "symfony/twig-bundle": "^3.4 || ^4.2",
        "symfony/web-server-bundle": "^3.4 || ^4.2"
    },
    "suggest": {
        "league/flysystem-bundle": "For Symfony >4.2 - Allow the use of the official adapter system instead of the compat layer."
    },
    "autoload": {
        "psr-4": {
            "DocusignBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DocusignBundle\\Tests\\": "tests/",
            "DocusignBundle\\E2e\\": "features/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
