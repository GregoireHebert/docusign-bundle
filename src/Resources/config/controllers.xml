<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="docusign.callback" class="DocusignBundle\Controller\Callback" public="true">
            <tag name="controller.service_arguments"/>
        </service>
        <service id="DocusignBundle\Controller\Callback" alias="docusign.callback" public="true" />

        <service id="docusign.webhook" class="DocusignBundle\Controller\Webhook" public="true">
            <tag name="controller.service_arguments"/>
        </service>
        <service id="DocusignBundle\Controller\Webhook" alias="docusign.webhook" public="true" />
    </services>
</container>
